#include "stm32f407xx.h"

void delay(void)
{
    for(uint32_t i = 0 ; i < 500000 ; i ++);
}


int main(void)
{

    GPIO_HandleTypeDef GpioLed;

    GpioLed.pGPIOx = GPIOD;
    GpioLed.Init.Pin = GPIO_PIN_12;
    GpioLed.Init.Mode = GPIO_MODE_OUT;
    GpioLed.Init.Speed = GPIO_SPEED_FAST;
    GpioLed.Init.OPType = GPIO_OP_TYPE_OD;
    GpioLed.Init.Pull = GPIO_NO_PUPD;

    GPIO_Init(&GpioLed);

    while(1)
    {
        GPIO_Togglein(GPIOD,GPIO_PIN_12);
        delay();
    }
    return 0;
}
